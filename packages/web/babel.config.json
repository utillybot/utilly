{
    "presets": [
        "@babel/env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": ["@babel/plugin-transform-runtime",
        ["@dr.pogodin/react-css-modules", {
            "generateScopedName": "[name]__[local]__[hash:base64:5]",
            "filetypes": {
                ".scss": {
                    "syntax": "postcss-scss"
                }
            }
        }]
    ]
}
